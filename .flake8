[flake8]
ignore = C101,D403,F403,F405,I100,I201,W503,W504,W191,Q000
exclude =
    **/migrations/*,
    .git,
    .mypy_cache,
    __pycache__,
    venv,
		.vscode,
		.py_env
filename = *.py
accept-encodings = utf-8
inline-quotes = single
max-complexity = 12
max-linenumber = 500
max-line-length = 119
multiline-quotes = double
per-file-ignores =
    src/core/settings/templates/settings.dev.py:F821
